<h1>Welcome to Capacitor!</h1>

<section id="projects">
  <%- @projects.each do |project| %>
    <article>
      <%= link_to_new_log_entry(project) %>
      <header>
        <h3><%= project.name %></h3>
      </header>
      <main>
        <p><%= project.infinite_capacity? %></p>
      </main>
    </article>
  <% end %>
</section>